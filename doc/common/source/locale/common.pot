msgstr ""

#: ../common/glossary.rst:2118
msgid "Launchpad"
msgstr ""

#: ../common/glossary.rst:2121
msgid "The collaboration site for OpenStack."
msgstr ""

#: ../common/glossary.rst:2122
msgid "Layer-2 network"
